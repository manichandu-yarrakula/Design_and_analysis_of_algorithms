function(int n) { 
    if (n==1) 
       return; 
    for (int i=1; i<=n; i++) 
    { 
        for (int j=1; j<=n; j++) 
        { 
            printf("*"); 
            break; 
        }     } 


         int m=0;
         for (int i = 0; i < n; i++) {
             m=m+1;
  }
